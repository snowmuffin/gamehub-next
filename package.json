{
  "name": "scifi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run clean:macos && next build",
    "start": "next start",
    "lint": "next lint",
    "postcss": "sass ./public/assets/scss/style.scss ./public/assets/css/style.css && postcss ./public/assets/css/style.css -o ./public/assets/css/style.css",
    "sass": "sass ./public/assets/scss/:./public/assets/css/",
    "sass-min": "sass ./public/assets/scss/:./public/assets/css/ --style compressed",
    "clean:macos": "find . -name '._*' -delete && find . -name '.DS_Store' -delete",
    "clean:all": "npm run clean:macos && rm -rf node_modules/.cache && rm -rf .next",
    "prebuild": "npm run clean:macos",
    "pm2:deploy": "./deploy.sh",
    "pm2:restart": "./restart.sh",
    "pm2:setup": "./ec2-setup.sh",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop gamehub-next",
    "pm2:delete": "pm2 delete gamehub-next",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs gamehub-next",
    "yarn:install": "yarn install",
    "yarn:build": "yarn build",
    "yarn:start": "yarn start",
    "yarn:clean": "./yarn-clean.sh",
    "env:set": "./set-env.sh"
  },
  "dependencies": {
    "@mui/icons-material": "7.3.1",
    "@mui/material": "7.3.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@simonwep/pickr": "1.9.1",
    "apexcharts": "^5.3.3",
    "axios": "1.11.0",
    "chart.js": "^4.5.0",
    "d3-geo": "^3.1.1",
    "echarts": "5.6.0",
    "echarts-for-react": "3.0.3",
    "leaflet": "^1.9.4",
    "moment": "^2.30.1",
    "nanoid": "^5.0.9",
    "next": "15.4.6",
    "react": "^19.1.1",
    "react-apexcharts": "1.7.0",
    "react-bootstrap": "2.10.10",
    "react-color-palette": "7.3.1",
    "react-countdown": "2.3.6",
    "react-country-state-city": "1.1.12",
    "react-datepicker": "^8.4.0",
    "react-i18next": "^15.2.0",
    "react-leaflet": "^5.0.0",
    "react-range": "1.10.0",
    "react-range-slider-input": "3.2.1",
    "react-redux": "9.2.0",
    "react-select": "5.10.2",
    "redux-persist": "^6.0.0",
    "simplebar-react": "3.3.2",
    "suneditor": "2.47.6",
    "swiper": "11.2.10",
    "yet-another-react-lightbox": "3.25.0"
  },
  "devDependencies": {
    "@types/node": "24.2.1",
    "@types/react": "19.1.10",
    "eslint-config-next": "15.4.6"
  }
}
