# EC2 Environment Variables Configuration File
# Copy this file to .env.production for use

# Next.js Environment Configuration
NODE_ENV=production
PORT=3000

# API Server URL (fallback for when rewrite doesn't work)
NEXT_PUBLIC_API_URL=https://REDACTED_API

# Steam Authentication URL (configured for EC2 environment)
NEXT_PUBLIC_STEAM_AUTH_URL=https://REDACTED_API/auth/steam

# Add other environment variables here
# DATABASE_URL=your_database_url
# SECRET_KEY=your_secret_key
